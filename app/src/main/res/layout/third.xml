<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/third"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFF2CC"
    tools:context=".frontend.Third">

    <ImageView
        android:layout_width="0dp"
        android:layout_height="375dp"
        app:srcCompat="@drawable/clearimage"
        android:id="@+id/imageView4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/logo"
        android:id="@+id/imageView5"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_width="0dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/imageView4" />

    <ImageButton
        android:layout_width="88dp"
        android:layout_height="88dp"
        app:srcCompat="@drawable/device_new"
        android:id="@+id/imageButton4"
        android:background="@drawable/circular_button"
        android:scaleType="fitCenter"
        app:layout_constraintRight_toRightOf="@+id/imageView4"
        app:layout_constraintLeft_toLeftOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintBottom_toTopOf="@+id/imageButton5"
        android:layout_marginBottom="16dp"
        android:layout_marginTop="8dp" />

    <ImageButton
        android:layout_height="88dp"
        app:srcCompat="@drawable/wlan_new"
        android:id="@+id/imageButton5"
        android:background="@drawable/circular_button"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        android:layout_marginBottom="32dp"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        android:layout_marginTop="250dp"
        app:layout_constraintRight_toRightOf="@+id/imageView4"
        app:layout_constraintLeft_toLeftOf="@+id/imageView4"
        android:layout_width="88dp" />

    <ImageView
        app:srcCompat="@drawable/ondevice"
        android:id="@+id/imageView2"
        app:layout_constraintBottom_toTopOf="@+id/imageButton4"
        android:layout_width="0dp"
        app:layout_constraintRight_toRightOf="@+id/imageView4"
        app:layout_constraintLeft_toLeftOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintVertical_bias="1.0"
        android:layout_height="50dp" />

    <ImageView
        android:layout_width="0dp"
        app:srcCompat="@drawable/onwlan"
        android:id="@+id/imageView3"
        app:layout_constraintBottom_toTopOf="@+id/imageButton5"
        app:layout_constraintRight_toRightOf="@+id/imageView4"
        app:layout_constraintLeft_toLeftOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageButton4"
        app:layout_constraintVertical_bias="1.0"
        android:layout_height="48dp" />


</android.support.constraint.ConstraintLayout>